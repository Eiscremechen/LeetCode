class Solution {
public:
    int maximumDifference(vector<int>& nums) {
        int premin = nums[0];
        int n = nums.size();
        int res = -1;
        for(int i = 0; i < n; i++){
            if(premin < nums[i]){
                res = max(nums[i] - premin, res);
            }
            else {
                premin = nums[i];
            }
        }
        return res;
    }
};
